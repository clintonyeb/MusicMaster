<div id="${album.artistId}">
    <a href="${album.url}">
        <div class="thumbnail">
            <img src="${album.artWorkUrl}" alt="Thumbnail" class="thumbnail" />
            <div class="caption">
                <h3 class="centered lead">${album.artistName}</h3>
                <p class="centered">${album.collectionName}</p>
                <p class="text-right">${album.primaryGenreName}</p>
            </div>
        </div>
    </a>

</div>